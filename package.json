{
  "name": "gorra-bot",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start -p tsconfig.json",
    "start:dev": "yarn start --watch",
    "start:debug": "yarn start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest --config jest.config.json",
    "test:watch": "jest --config jest.config.json --watch",
    "test:cov": "jest --config jest.config.json --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "commit": "git add . && git-cz"
  },
  "dependencies": {
    "@nest-middlewares/morgan": "^6.0.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^6.7.2",
    "@nestjs/mongoose": "^7.0.0",
    "@nestjs/platform-express": "^6.7.2",
    "@slack/web-api": "^5.10.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "mongoose": "^5.9.13",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^24.0.18",
    "@types/mongoose": "^5.7.18",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "commitizen": "^4.1.2",
    "git-cz": "^4.3.1",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "lint-staged": "^10.2.2",
    "mongodb-memory-server": "^6.6.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
